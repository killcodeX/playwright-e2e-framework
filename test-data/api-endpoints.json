{
    "baseUrl": "https://api.example.com",
    "endpoints": {
        "auth": {
            "login": "/auth/login",
            "logout": "/auth/logout",
            "register": "/auth/register"
        },
        "users": {
            "list": "/users",
            "profile": "/users/:id",
            "update": "/users/:id"
        },
        "products": {
            "list": "/products",
            "details": "/products/:id",
            "search": "/products/search"
        }
    },
    "headers": {
        "contentType": "application/json",
        "accept": "application/json"
    }
}